# Videame Admin Panel Configuration
# 1. Set a secure password for the /admin route
NEXT_PUBLIC_ADMIN_PASSWORD="YOUR_SECURE_PASSWORD_HERE"

# Netlify API Configuration for Casting Form
# 2. Get your Form ID from your Netlify site > Forms > (your form) > Form details
NEXT_PUBLIC_NETLIFY_FORM_ID="YOUR_NETLIFY_FORM_ID_HERE"
# 3. Get your Personal Access Token from Netlify User settings > Applications > Personal access tokens
NEXT_PUBLIC_NETLIFY_API_TOKEN="YOUR_NETLIFY_PERSONAL_ACCESS_TOKEN_HERE"

# Firebase Configuration (if you still use other Firebase services)
NEXT_PUBLIC_FIREBASE_API_KEY=YOUR_VALUE
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=YOUR_VALUE
NEXT_PUBLIC_FIREBASE_PROJECT_ID=YOUR_VALUE
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=YOUR_VALUE
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=YOUR_VALUE
NEXT_PUBLIC_FIREBASE_APP_ID=YOUR_VALUE
